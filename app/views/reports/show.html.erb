<p id="notice"><%= notice %></p>

<p>
  <strong>Starting at:</strong>
  <%= @report.starting_at %>
</p>

<p>
  <strong>Ending at:</strong>
  <%= @report.ending_at %>
</p>

<% @event_counts[0..24].each_with_index do |event, index| %>
  <%= index+1 %>. <%= event[0][0] %> | <%= event[0][1] %>  | <%= event[1] %> <br>
<% end %>
<br><br>

<% @event_counts[0..9].each_with_index do |event, index| %>
  <%= index+1 %>. <%= event[0][0] %> | <%= event[1] %>  <br>
<% end %>
<br><br>

<p>
  <%= pluralize(@reports_push_events.count, 'event') %>
</p>
<% @reports_push_events.each do |event| %>
  <%= event.the_type %> | event.time_created %> | <%= event.repo_name %> | <%= event.repo_url %><br>
<% end %>


<%= link_to 'Edit', edit_report_path(@report) %> |
<%= link_to 'Back', reports_path %>
